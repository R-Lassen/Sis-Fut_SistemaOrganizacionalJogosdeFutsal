@model UsuarioModel
@{
    ViewData["Title"] = "Confirmação apagar Usuário";
}

<div class="text-center">
    <h3>Realmente dejeja apagar o usuário @Model.Name?</h3>
    
</div>


<a class="btn btn-primary" asp-action="Index" asp-asp-controller="Usuario" role="button">Cancelar Exclusão </a>
<a class="btn btn-danger" asp-action="Apagar" asp-asp-controller="Usuario" asp-route-id="@Model.Id" role="button">Confirmar exclusão </a>

