@model ContatoModel
@{
    ViewData["Title"] = "Confirmação apagar contatos";
}

<div class="text-center">
    <h3>Realmente dejeja apagar o contato @Model.Nome?</h3>
    
</div>


<a class="btn btn-primary" role="button" asp-asp-controller="Contato" asp-action="Index">Cancelar Exclusão </a>
<a class="btn btn-danger" role="button" asp-asp-controller="Contato" asp-route-id="@Model.Id" asp-action="Apagar">Confirmar Exclusão </a>

